    gem "bulma-clean-theme"
    gem "nokogiri"
    gem "jekyll-paginate-v2"
    gem "jekyll-feed", github: "jekyll/jekyll-feed"
    gem "jekyll-seo-tag"
    gem 'jekyll-redirect-from'
end
